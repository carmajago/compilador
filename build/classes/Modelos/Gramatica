LP ::= <page> HEAD_NO BODY_NO </page> CLASES_NO USUARIOS_NO
HEAD_NO ::= <head>  TITULO_NO LIBRERIAS_NO INFO_NO CRUD </head>

CRUD ::= CREATE_NO | READ_NO | UPDATE_NO | DELETE_NO |
CREATE_NO ::= create CADENA values(VALORES_NO);
READ_NO ::=  read CADENA values(VALORES_NO);
UPDATE_NO ::= update CADENA values(VALORES_NO);
DELETE_NO ::= delete CADENA values(VALORES_NO);
VALORES_NO::=CADENA VALORES_NO|

TITULO_NO ::= title = CADENA ;
LIBRERIAS_NO ::= include ( RUTA_NO ) |
INFO_NO ::= CADENA |

BODY_NO ::= <body> (ATRIBUTOS_NO) COMPONENTES_NO SECCIONES_NO </body>
SECCIONES_NO ::= SECCION_NO SECCIONES_NO |
SECCION_NO ::= <div> (ATRIBUTOS_NO) COMPONTES_NO SECCIONES_NO </div> 
COMPONTES_NO :: COMPONTE_NO COMPONTES_NO |
COMPONTE_NO ::=  H1_NO | H2_NO | P_NO | INPUT_NO | IMAGEN_NO | BOTON_NO  | CHECK_NO | LISTA_NO |
H1_NO ::= h1(ATRIBUTOS_NO){ CADENA }
H2_NO ::= h2(ATRIBUTOS_NO){ CADENA }
P_NO ::= p(ATRIBUTOS_NO){ CADENA }
INPUT_NO ::= input(ATRIBUTOS_NO TIPO_NO){ CADENA }
TIPO_NO ::= text | number | password |
IMAGEN_NO ::= image (RUTA_NO ATRIBUTOS_NO)
BOTON_NO ::= button(ATRIBUTOS_NO){ CADENA }
CHECK_NO ::= check(ATRIBUTOS_NO){ CADENA }
LISTA_NO ::=  list ( ATRIBUTOS_NO ) { ITEMS_NO }
ITEMS_NO ::= CADENA ITEMS_NO |

ATRIBUTOS_NO ::= ANCHO_NO ALTO_NO ESTILOS_NO FUNCIONES_NO ID_NO
ID_NO ::= id = CADENA
ANCHO_NO ::= witdh = NUMERICO
ALTO_NO ::= height = NUMERICO
ESTILOS_NO ::= style = CADENA |
FUNCIONES_NO ::= METODOS = CADENA |
METODOS ::= onload | onClick | focus

CADENA ::= [A-Z]
NUMERICO ::= [0-9]*
ALFANUMERICO= CADENA NUMERICO | NUMERICO CADENA

CLASES_NO ::= CLASE_NO CLASES_NO |
CLASE_NO ::= class CADENA {COLUMNAS_NO RESTRICCIONES_NO}
RESTRICCIONES_NO ::= primary key CADENA;  
COLUMNAS_NO::= COLUMNA_NO (NUMERICO) COLUMNAS_NO |
COLUMNA_NO ::= TIPOVAR_NO  CADENA ;
TIPOVAR_NO ::= number | varchar2 | bit |char

USUARIOS_NO ::= USUARIO_NO USUARIOS_NO |
USUARIO_NO ::= user PERMISOS_NO {userid=CADENA password=ALFANUMERICO }
PERMISOS_NO ::= read | write |  
